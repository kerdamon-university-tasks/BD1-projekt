<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>

<body>

  <%- include('../partials/nav'); %>
  <%- include('../partials/header', {headerText: `Aktualizowanie danych w tabeli obecnosc`}); %>
  
  <div class="container">
    <div class="jumbotron">
      <form action="/db/updateObecnosc" method="POST">
          <div class="form-group">
            <label for="czlonek_id">czlonek_id:</label>
            <input type="text" class="form-control" id="czlonek_id" name="czlonek_id">
          </div>
          <div class="form-group">
            <label for="spotkanie_id">spotkanie_id:</label>
            <input type="text" class="form-control" id="spotkanie_id" name="spotkanie_id">
          </div>
          <div class="form-group">
            <label for="oplacono_skladke">oplacono_skladke:</label>
            <input type="text" class="form-control" id="oplacono_skladke" name="oplacono_skladke">
          </div>
        <button type="submit" class="btn btn-primary">Zaktualizuj</button>
      </form>
    </div>
    <div class="jumbotron">
      <% tables.forEach(table => { %>
        <h2><%= table.tableName %></h2>
        <%- include('../partials/db-basic-table.ejs', {results: table.results}); %>
      <% }) %>
    </div>
  </div>

</body>
</html>