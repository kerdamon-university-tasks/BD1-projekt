<% if (!(field.name === commonFieldName)) { %>
  <div class="form-group">
    <label for="<%= field.name %>"><%= field.name %>:</label>

    <% if (field.name === secondPkFieldName) { %>
      <input type="text" class="form-control" id="<%= value %>_<%= field.name %>" value="<%= value %>" name="<%= field.name %>" disabled>
    <% } else if (field.state === 'normal') { %>
      <input type="text" class="form-control" id="<%= value %>_<%= field.name %>" name="<%= field.name %>">
    <% } else if (field.state === 'autonumerated') { %>
      <input type="text" class="form-control" placeholder="Automatyczne numerowanie" id="<%= field.name %>" disabled>
    <% } %>
  </div>
<% } %>