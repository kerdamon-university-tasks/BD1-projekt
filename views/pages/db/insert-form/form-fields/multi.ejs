<% if (!(field.name === commonFieldName)) { %>
  <div class="form-group">

    <%
    let fdata = {
      fname:`${tableName}-${field.name}_${String(value).padStart(5, '0')}`,
      flabel: `${field.name}`
    } 

    if (field.type === 16){
      fdata.ftype = 'checkbox';
      fdata.fvalue = 'true';
    } else {
      fdata.ftype = 'text';
    }

    if (field.name === secondPkFieldName) {
      fdata.fdisabled = 'disabled';
      fdata.fvalue = value;
    }
    %>

    <%- include('input', fdata) %> 
  </div>
<% } %>